<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="primaryPane" prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #858DF1;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SearchUsersController">
   <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#464eb8" height="46.0" layoutY="-3.0" stroke="BLACK" strokeType="INSIDE" width="800.0" />
   <Label alignment="CENTER" layoutX="246.0" layoutY="6.0" prefHeight="18.0" prefWidth="306.0" text="Search User" textFill="WHITE">
      <font>
         <Font name="Times New Roman Bold" size="23.0" />
      </font>
   </Label>
   <TextField fx:id="searchField" layoutX="217.0" layoutY="58.0" opacity="0.88" prefHeight="26.0" prefWidth="306.0" promptText="Search User" style="-fx-background-color: White; -fx-border-color: #ad3dea;">
      <font>
         <Font name="Times New Roman" size="15.0" />
      </font>
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor>
      <effect>
         <Glow />
      </effect>
   </TextField>
   <Button fx:id="searchButton" layoutX="550.0" layoutY="58.0" mnemonicParsing="false" onAction="#searchButtonPressed" prefHeight="31.0" prefWidth="94.0" text="Search" />
   <ListView fx:id="userListView" layoutX="217.0" layoutY="89.0" prefHeight="264.0" prefWidth="306.0" style="-fx-background-color: Transparent;" />
   <Button fx:id="backButton" layoutX="14.0" layoutY="7.0" mnemonicParsing="false" onAction="#onButtonClicked" text="Back" />
</AnchorPane>
